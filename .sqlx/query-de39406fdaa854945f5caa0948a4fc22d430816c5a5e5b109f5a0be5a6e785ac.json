{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                id,\n                user_id,\n                status as \"status: _\",\n                from_email,\n                recipients,\n                NULL::bytea AS \"raw_data\",\n                NULL::jsonb AS \"message_data\",\n                created_at,\n                updated_at\n            FROM messages\n            WHERE ($3::message_status IS NULL OR status = $3)\n            AND ($4::uuid IS NULL OR user_id = $4)\n            ORDER BY created_at DESC\n            OFFSET $1\n            LIMIT $2\n            ",
