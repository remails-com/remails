  "query": "\n            SELECT s.id,\n                   s.project_id,\n                   s.name,\n                   s.created_at,\n                   s.updated_at\n            FROM streams s\n                JOIN projects p ON s.project_id = p.id \n                     WHERE s.project_id = $1 \n                       AND p.organization_id = $2 \n            ",
