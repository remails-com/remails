{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                m.id,\n                m.organization_id,\n                m.domain_id,\n                m.project_id,\n                m.stream_id,\n                m.smtp_credential_id,\n                m.status as \"status: _\",\n                m.from_email,\n                m.recipients,\n                m.raw_data,\n                m.message_data,\n                m.created_at,\n                m.updated_at\n            FROM messages  m\n                JOIN organizations o ON o.id = m.organization_id\n            WHERE m.id = $1\n              AND ($2::uuid[] IS NULL OR o.id = ANY($2))\n            LIMIT 1\n            ",
